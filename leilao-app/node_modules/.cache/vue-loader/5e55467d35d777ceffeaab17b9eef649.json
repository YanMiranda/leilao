   sort-by=\"ID\"\n      :item=\"customRowStyle\"\n    >\n      <template v-slot:top>\n        <v-toolbar flat class=\"pa-5\">\n            <v-toolbar-title class=\"title-table\">{{ title }}</v-toolbar-title>\n            <v-spacer></v-spacer>\n            <v-btn\n              outlined\n              elevation=\"2\"\n              color=\"success\"\n              @click=\"createItem\"\n            >\n              <v-icon small>mdi-plus</v-icon>\n              Adicionar\n            </v-btn>\n            <v-btn\n              outlined\n              class=\"ml-2\"\n              elevation=\"2\"\n              color=\"primary\"\n              @click=\"serachItem\"\n            >\n              <v-icon small>mdi-magnify</v-icon>\n              Buscar\n            </v-btn>\n        </v-toolbar>\n        <v-divider></v-divider>\n      </template>\n      <div></div>\n        <template v-slot:[`item.actions`]=\"{ item }\">\n          <v-icon\n            small\n            class=\"mr-2\"\n            @click=\"editRow(item)\"\n          >\n            mdi-pencil\n          </v-icon>\n          <v-icon\n            small\n            @click=\"deleteRow(item)\"\n          >\n            mdi-delete\n          </v-icon>\n        </template>\n    </v-data-table>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'TabelaDinamica',\n\n  props: {\n    title: {\n      type: String,\n      required: true\n    },\n    headers: {\n      type: Array,\n      required: true,\n    },\n    items: {\n      type: Array,\n      required: true,\n    },\n    showbtnColumn: {\n      type: Boolean,\n      default: true,\n    },\n    showBtnAdd: {\n      type: Boolean,\n      default: false,\n    },\n    showSeachButton: {\n      type: Boolean,\n      default: false,\n    },\n  },\n\n  methods: {\n    customRowStyle(item) {\n      console.log(item);\n      return {\n        'background-color': 'lightblue' // Defina a cor de fundo desejada\n      };\n    },\n    editRow(index, item) {\n      this.$emit('onEdit', index, item);\n    },\n\n    deleteRow(item) {\n      this.$emit('onDelete', item);\n    },\n\n    createItem() {\n      this.$emit('onAdd');\n    },\n\n    serachItem() {\n      this.$emit('onSearch');\n    },\n  },\n};\n</script>\n\n<style scoped>\n.title-table {\n  margin-bottom: 40px;\n  text-align: center;\n  color: #138496;\n  margin-top: 20px;\n  font-size: xx-large;\n}\n\n.custom-table .v-data-table__row {\n  color: #138496;\n}\n</style>\n"]}]}